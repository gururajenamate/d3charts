<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" xmlns="sap.m"
		controllerName="d3charts.controller.Main" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="HANA Growth QoQ by Deployment Type and Q2 Pipeline by Deployment Type" class="sapUiFioriObjectPage"
		showNavButton="{device>/isPhone}" navButtonPress="handleNavButtonPress"
		navButtonTap="onBackButtonPress">
		<l:VerticalLayout class="panelBoxContainer sapUiFioriObjectPage"
			width="100%">
			<l:content>
				<Panel>
					<headerToolbar>
						<Toolbar>
							<Label text="Closing Quarter by Region (W+B KEur)" />
						</Toolbar>
					</headerToolbar>
					<FlexBox id="RegionStackedBarChartHolder" alignItems="Start"
						justifyContent="Center"></FlexBox>
				</Panel>
				<Panel>
					<headerToolbar>
						<Toolbar>
							<Label text="Closing Quarter by Region (W+B KEur ~ Deal Count)" />
						</Toolbar>
					</headerToolbar>
					<FlexBox id="RegionScatterPlotHolder" alignItems="Start"
						justifyContent="Center"></FlexBox>
				</Panel>
				<Panel>
					<headerToolbar>
						<Toolbar>
							<Label text="Closing Quarter by Segment (W+B KEur)" />
						</Toolbar>
					</headerToolbar>
					<FlexBox id="SegmentStackedBarChartHolder" alignItems="Start"
						justifyContent="Center"></FlexBox>
				</Panel>
				<Panel>
					<headerToolbar>
						<Toolbar>
							<Label text="Closing Quarter by Segment (W+B KEur ~ Deal Count)" />
						</Toolbar>
					</headerToolbar>
					<FlexBox id="SegmentScatterPlotHolder" alignItems="Start"
						justifyContent="Center"></FlexBox>
				</Panel>
			</l:content>
		</l:VerticalLayout>
	</Page>
</core:View>